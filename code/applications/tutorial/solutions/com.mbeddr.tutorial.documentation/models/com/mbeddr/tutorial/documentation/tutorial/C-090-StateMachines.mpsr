              <property name="escapedValue" nameId="87nw.2557074442922438158" value="state landing { \n  on next [tp-&gt;speed == 0 mps] -&gt; landed \n  on next [tp-&gt;speed != 0 mps ] -&gt; landing { points--; }  \n  on reset [ ] -&gt; beforeFlight \n} " />
            </node>
          </node>
        </node>
        <node role="contents" roleId="2c95.3350625596580064250" type="2c95.TextParagraph" typeId="2c95.3350625596580089586" id="3458996998997773028" nodeInfo="ng">
